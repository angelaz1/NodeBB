{
  "compilerOptions": {
    "allowJs": false,
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
  },
  "include": [
    "public/src/**/*.ts",
    "src/**/*.ts", 
    "test/**/*.ts",
  ],
  "exclude":[
    "node_modules",
    "src/types"
  ]
}